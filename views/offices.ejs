          <!--First row-->
          <div class="row">
            <div class="col-12 text-left">
              <h3 class="mb-3 feature-title"><strong> <%= services.servicetitle %> </strong></h3>
              <p class="mb-3 grey-text"> <%- services.servicedescription %> </p>
              <h5 class="mb-3 feature-title"><strong> <%= services.locationtitle %> </strong></h5>
              <img class="mb-3 img-fluid" src="uploads/locationimage.jpg" style="object-fit: cover; max-height: 300px; width: 90%"></img>
              <p class="mb-3 grey-text"> <%- services.locationdescription %> </p>
              <h5 class="mb-3 feature-title"><strong> <%= services.officetitle %> </strong></h5>
              <img class="mb-3 img-fluid" src="uploads/officeimage.jpg" style="object-fit: cover; max-height: 300px; width: 90%"></img>
              <p class="mb-3 grey-text">  <%- services.officedescription %> </p>
            </div>
          </div>
          <!--/First row-->

        </div>
        <!--/Grid column-->

        <!--Grid column-->

          <div class="col-lg-2 col-md-12">
            <div class="card rounded p-3">
            <!--First row-->
            <div class="row">

              <div class="col-12 text-center justify-content-center">
                <a href="" data-toggle="modal" data-target="#message">  <h5 class="mb-3 feature-title black-text"><strong>Contact Us</strong></h5>
                <i class="mb-3 fas fa-phone sd-bar-icons"></i> <a/>
                <p class="mb-3 grey-text"> <%- sidebars.contact %> </p>
                <a data-toggle="modal" data-target="#message" class="mb-3 btn btn-blue btn-sm rounded p-2">Contact Us</a>
              </div>
            </div>
            <!--/First row-->
            <!-- Second Row -->
            <div class="row">

              <div class="col-12 text-center">
                <hr class="my-3">
                <a href="/en/setup"> <h5 class="mb-3 feature-title black-text"><strong>Business Set-up?</strong></h5>
                <i class="mb-3 fas fa-handshake sd-bar-icons"></i> </a>
                <p class="mb-3 grey-text"> <%- sidebars.setup %> </p>
                <a target="" href="/en/setup" class="mb-3 btn btn-blue btn-sm rounded p-2">Find Out More</a>
              </div>
            </div>

            <!-- Second Row -->
            </div>
          </div>
          <!--/Grid column-->


      </div>
  </div>
